    {
      "Name": "InventoryPart",
      "Settings": {
        "ContentPartSettings": {
          "Attachable": true,
          "Description": "Adds basic inventory management capabilities to a product."
        }
      },
      "ContentPartFieldDefinitionRecords": [
        {
          "FieldName": "BooleanField",
          "Name": "AllowsBackOrder",
          "Settings": {
            "ContentPartFieldSettings": {
              "DisplayName": "Allows Back Order"
            },
            "BooleanFieldSettings": {
              "Hint": "Allows ordering even if Inventory is less than or equal to zero."
            }
          }
        },
        {
          "FieldName": "BooleanField",
          "Name": "IgnoreInventory",
          "Settings": {
            "ContentPartFieldSettings": {
              "DisplayName": "Ignore Inventory"
            },
            "BooleanFieldSettings": {
              "Hint": "Makes it so Inventory is ignored (same as if no InventoryPart was present). Useful for digital products for example."
            }
          }
        },
        {
          "FieldName": "NumericField",
          "Name": "MaximumOrderQuantity",
          "Settings": {
            "ContentPartFieldSettings": {
              "DisplayName": "Maximum Order Quantity"
            },
            "NumericFieldSettings": {
              "Hint": "The maximum number of this item one can order. Ignored if set to zero or a negative value."
            }
          }
        },
        {
          "FieldName": "NumericField",
          "Name": "MinimumOrderQuantity",
          "Settings": {
            "ContentPartFieldSettings": {
              "DisplayName": "Minimum Order Quantity"
            },
            "NumericFieldSettings": {
              "Hint": "The minimum number of this item one can order. Ignored if set to zero or a negative value."
            }
          }
        },
        {
          "FieldName": "HtmlField",
          "Name": "OutOfStockMessage",
          "Settings": {
            "ContentPartFieldSettings": {
              "DisplayName": "Out of Stock Message",
              "Editor": "Multiline"
            },
            "HtmlFieldSettings": {
              "Hint": "Enables a specific message for an out of stock product. Can be used to give an ETA."
            }
          }
        }
      ]
    },
    {
      "Name": "DiscountPart",
      "Settings": {
        "ContentPartSettings": {
          "Attachable": true,
          "Reusable": true,
          "Description": "Adds configurable discount to the product."
        }
      },
      "ContentPartFieldDefinitionRecords": [
        {
          "FieldName": "NumericField",
          "Name": "DiscountPercentage",
          "Settings": {
            "ContentPartFieldSettings": {
              "DisplayName": "Discount Percentage"
            },
            "NumericFieldSettings": {
              "Hint": "The discount percentage that is applied to the product. Out of discount percentage and discount amount only one can be used.",
              "Minimum": 0.0,
              "Maximum": 100.0
            }
          }
        },
        {
          "FieldName": "PriceField",
          "Name": "DiscountAmount",
          "Settings": {
            "ContentPartFieldSettings": {
              "DisplayName": "Discount Amount"
            },
            "PriceFieldSettings": {
              "Hint": "The discount amount that is subtracted from the product's price. Out of discount percentage and discount amount only one can be used."
            }
          }
        },
        {
          "FieldName": "DateTimeField",
          "Name": "BeginningUtc",
          "Settings": {
            "ContentPartFieldSettings": {
              "DisplayName": "Beginning"
            },
            "DateTimeFieldSettings": {
              "Hint": "The beginning date and time of the discount."
            }
          }
        },
        {
          "FieldName": "DateTimeField",
          "Name": "ExpirationUtc",
          "Settings": {
            "ContentPartFieldSettings": {
              "DisplayName": "Expiration"
            },
            "DateTimeFieldSettings": {
              "Hint": "The expiration date and time of the discount."
            }
          }
        },
        {
          "FieldName": "NumericField",
          "Name": "MaximumProducts",
          "Settings": {
            "ContentPartFieldSettings": {
              "DisplayName": "Maximum Products"
            },
            "NumericFieldSettings": {
              "Hint": "The amount of maximum products that the discount can be applied to. If the quantity is higher than this, the discount is ignored. Type 0 for infinite products.",
              "Minimum": 0.0
            }
          }
        },
        {
          "FieldName": "NumericField",
          "Name": "MinimumProducts",
          "Settings": {
            "ContentPartFieldSettings": {
              "DisplayName": "Minimum Products"
            },
            "NumericFieldSettings": {
              "Hint": "The amount of minimum products that the discount can be applied to. If the quantity is lower than this, the discount is ignored.",
              "Minimum": 0.0
            }
          }
        }
      ]
    },
