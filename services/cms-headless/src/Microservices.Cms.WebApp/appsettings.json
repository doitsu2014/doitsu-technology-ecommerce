{
  "OrchardCore": {
    "OrchardCore_Shells_Database": {
      "DatabaseProvider": "SqlConnection",
      "ConnectionString": "",
      "TablePrefix": "",
      "MigrateFromFiles": true
    },
    "OrchardCore_Redis": {
      "Configuration": "redis://:aaa@localhost:6379,abortConnect=false,allowAdmin=true"
    },
    "OrchardCore_AutoSetup": {
      "AutoSetupPath": "",
      "LockOptions": {
        "LockTimeout": "10000",
        "LockExpiration": "10000"
      },
      "Tenants": [
        {
          "ShellName": "Default",
          "SiteName": "Doitsu Technology",
          "SiteTimeZone": "Asia/Bangkok",
          "AdminUsername": "admin",
          "AdminEmail": "admin@dtech.com",
          "AdminPassword": "",
          "DatabaseProvider": "SqlConnection",
          "DatabaseConnectionString": "",
          "DatabaseTablePrefix": "",
          "RecipeName": "SaaS"
        }
      ]
    },
    "OrchardCore_Google": {
      "ClientID": "",
      "ClientSecret": "",
      "CallbackPath": "/signin-google",
      "SaveTokens": false
    },
    "OrchardCore_GitHub": {
      "ClientID": "",
      "ClientSecret": "",
      "CallbackPath": "/signin-github",
      "SaveTokens": false
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Default": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
          "outputTemplate": "{Timestamp:HH:mm:ss}|{TenantName}|{RequestId}|{SourceContext}|{Level:u3}|{Message:lj}{NewLine}{Exception}",
          "restrictedToMinimumLevel": "Information"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "App_Data/logs/orchard-log.txt",
          "rollingInterval": "Day",
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.ffff}|{TenantName}|{MachineName}|{RequestId}|{SourceContext}|{Level:u3}|{Message:lj}{NewLine}{Exception}",
          "restrictedToMinimumLevel": "Warning"
        }
      }
    ]
  }
}
